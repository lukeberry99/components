{
  "name": "webiik/components",
  "description": "Webiik components for lean web development with minimal dependencies.",
  "keywords": ["components", "webiik"],
  "homepage": "https://www.webiik.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Jiri Mihal",
      "email": "jiri@mihal.me"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "replace": {
    "webiik/arr": "self.version",
    "webiik/container": "self.version",
    "webiik/cookie": "self.version",
    "webiik/csrf": "self.version",
    "webiik/curlhttpclient": "self.version",
    "webiik/data": "self.version",
    "webiik/database": "self.version",
    "webiik/error": "self.version",
    "webiik/flash": "self.version",
    "webiik/log": "self.version",
    "webiik/login": "self.version",
    "webiik/mail": "self.version",
    "webiik/middleware": "self.version",
    "webiik/oauth1client": "self.version",
    "webiik/oauth2client": "self.version",
    "webiik/router": "self.version",
    "webiik/session": "self.version",
    "webiik/token": "self.version",
    "webiik/translation": "self.version",
    "webiik/validator": "self.version",
    "webiik/view": "self.version"
  },
  "require-dev": {
    "swiftmailer/swiftmailer": "^6.0",
    "phpmailer/phpmailer": "~6.0",
    "pimple/pimple": "^3",
    "twig/twig": "^2",
    "webiik/cli-analyse": "*"
  },
  "autoload": {
    "psr-4": {
      "Webiik\\": "src/Webiik"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
